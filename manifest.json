{
  "manifest_version": 2,

  "name": "MoarTabs",
  "description": "10 tabs are too little for me",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": ["storage", "tabs"],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
